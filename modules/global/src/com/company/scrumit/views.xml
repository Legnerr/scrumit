<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">
    <view class="com.company.scrumit.entity.Contact"
          extends="_local"
          name="contact-view">
        <property name="specialities"
                  view="_minimal"/>
        <property name="city"
                  view="_minimal"/>
    </view>
    <view class="com.company.scrumit.entity.Task"
          extends="_local"
          name="tasks-full">
        <property name="performer"
                  view="_minimal"/>
        <property name="task"
                  view="_minimal">
            <property name="level"/>
            <property name="top"
                      view="_minimal"/>
        </property>
        <property name="duration"/>
        <property name="top"
                  view="_minimal"/>
    </view>
    <view class="com.company.scrumit.entity.Task"
          extends="_minimal"
          name="task-tree">
        <property name="task"
                  view="_minimal"/>
        <property name="proirity"/>
    </view>
    <view class="com.company.scrumit.entity.Team"
          extends="_local"
          name="team-view">
        <property name="leader"
                  view="_minimal"/>
        <property name="projects"
                  view="_minimal"/>
        <property name="members"
                  view="_minimal">
            <property name="email"/>
        </property>
    </view>
</views>
